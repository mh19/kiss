{
    "name": "KISS devstack",
    "description": "The most innovative PHP devstack standing on the shoulder of giants",
    "repository": "https://github.com/plispe/kiss",
    "env": {
        "DISPLAY_ERRORS": {
            "value": "false"
        },
        "USE_DI_CACHE": {
            "value": "false"
        },
        "USE_DOTENV_FILE": {
            "value": "false"
        },
        "USE_LATTE_CACHE": {
            "value": "true"
        }
    },
    "compile": [
        "composer global require hirak/prestissimo"
    ]
}
